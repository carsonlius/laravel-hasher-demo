{
  "name": "carsonlius/hasher",
  "description": "MD6 support in laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "carsonlius",
      "email": "carsonlius@163.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php" : ">=7.0.0"
  },
  "autoload": {
    "psr-4": {
      "Carsonlius\\Hasher\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Carsonlius\\Hasher\\MD5HasherProvider"
      ],
      "aliases": {
        "Md5Hasher": "Carsonlius\\Hasher\\Facade"
      }
    }
  }
}
